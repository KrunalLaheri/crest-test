				{
					"name": "Register Admin User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 201) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"access_token\", jsonData.tokens.access);",
									"    pm.environment.set(\"refresh_token\", jsonData.tokens.refresh);",
									"    console.log(\"Admin user registered successfully!\");",
									"    console.log(\"Access token saved to environment\");",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"admin1@test.com\",\n    \"username\": \"admin1\",\n    \"password\": \"Admin12345\",\n    \"password2\": \"Admin12345\",\n    \"role\": \"admin\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/auth/register/",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"auth",
								"register",
								""
							]
						},
						"description": "Register a new admin user. Returns user data and JWT tokens.\n\n**Required Fields:**\n- email: Valid email address\n- username: Unique username\n- password: Strong password\n- password2: Password confirmation\n- role: \"admin\" or \"user\"\n\n**Response:**\n- User data\n- JWT access token (1 hour expiry)\n- JWT refresh token (1 day expiry)"
					},
					"response": []
				},
